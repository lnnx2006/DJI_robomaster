#include "ros/ros.h"
#include "SerialNode_base.h"
#include "SerialPort.h"

using namespace std;
using namespace XM_SerialNode;

int main(int argc, char **argv)
{
	ros::init(argc, argv, "Gimbal_SerialNode");

	SerialNode serialnode(ros::NodeHandle(), ros::NodeHandle("~"));

	ros::spin();

	return 0;
}


